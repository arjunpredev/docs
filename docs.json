{
	"$schema": "https://mintlify.com/docs.json",
	"theme": "mint",
	"name": "pre.dev Architect API",
	"appearance": {
		"default": "dark"
	},
	"colors": {
		"primary": "#000000",
		"light": "#FFFFFF",
		"dark": "#333333"
	},
	"favicon": "https://pre.dev/favicon.ico",
	"api": {
		"baseUrl": "https://api.pre.dev",
		"auth": {
			"method": "key",
			"name": "x-api-key",
			"inputPrefix": ""
		},
		"playground": {
			"mode": "show"
		},
		"maintainOrder": true
	},
	"openapi": ["api-reference/openapi.json"],
	"navigation": {
		"tabs": [
			{
				"tab": "Documentation",
				"groups": [
					{
						"group": "Get Started",
						"pages": ["index", "why-architect-api", "choosing-spec-level"]
					}
				]
			},
			{
				"tab": "SDKs",
				"groups": [
					{
						"group": "Official SDKs",
						"pages": ["sdks/overview", "sdks/python", "sdks/node"]
					}
				]
			},
			{
				"tab": "API Reference",
				"groups": [
					{
						"group": "Using the API",
						"pages": ["api-reference/introduction"]
					},
					{
						"group": "Spec Endpoints",
						"pages": ["api-reference/fast-spec", "api-reference/deep-spec"]
					},
					{
						"group": "Status Endpoints",
						"pages": ["api-reference/spec-status"]
					}
				]
			}
		],
		"global": {
			"anchors": [
				{
					"anchor": "Create a Project",
					"href": "https://pre.dev",
					"icon": "plus"
				},
				{
					"anchor": "Find Developers",
					"href": "https://pre.dev/directory",
					"icon": "user"
				},
				{
					"anchor": "API Playground",
					"href": "https://pre.dev/projects/playground",
					"icon": "code"
				},
				{
					"anchor": "Blog",
					"href": "https://pre.dev/blog",
					"icon": "newspaper"
				},
				{
					"anchor": "Community",
					"href": "https://discord.com/invite/ejVTRJ6WXS",
					"icon": "discord"
				}
			]
		}
	},
	"logo": {
		"light": "https://pre.dev/predev_logo_name_white.png",
		"dark": "https://pre.dev/predev_logo_name.png"
	},
	"navbar": {
		"links": [
			{
				"label": "pre.dev",
				"href": "https://pre.dev"
			}
		],
		"primary": {
			"type": "button",
			"label": "Get API Key",
			"href": "https://pre.dev/projects/playground"
		},
		"logoHeight": "60px"
	},
	"contextual": {
		"options": [
			"copy",
			"view",
			"chatgpt",
			"claude",
			"perplexity",
			"mcp",
			"cursor",
			"vscode"
		]
	},
	"footer": {
		"socials": {
			"linkedin": "https://www.linkedin.com/company/99516177/",
			"x": "https://x.com/predotdev",
			"youtube": "https://www.youtube.com/@predotdev",
			"instagram": "https://www.instagram.com/predev.ai/",
			"discord": "https://discord.gg/ejVTRJ6WXS"
		}
	}
}
